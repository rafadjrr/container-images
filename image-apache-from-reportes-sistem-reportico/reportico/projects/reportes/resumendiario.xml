(SELECT a.Codigo_Cuenta, b.Desc_Cuenta,
CASE WHEN a.Cargo_Abono = 1 THEN sum(a.Monto) ELSE 0 END AS TotDeb,
CASE WHEN a.Cargo_Abono = 0 THEN sum(a.Monto) ELSE 0 END AS TotCred
FROM  dt01asco a join en01cuen b on a.Codigo_Cuenta=b.Codigo_Cuenta
WHERE  (1 = 1) [ AND (a.Codigo_Cuenta between {desdecodigo} and {hastacodigo} ) ]
[ AND Fecha between {fechadesde} and {fechahasta} ]
GROUP BY a.Codigo_Cuenta,a.Cargo_Abono, b.Desc_Cuenta) z GROUP BY z.Codigo_Cuenta, z.Desc_Cuenta ORDER BY z.Codigo_Cuenta
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Codigo_Cuenta</Name><TableName>z</TableName><ColumnName>Codigo_Cuenta</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>12%</ColumnWidthHTML><column_title>Cuenta</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Desc_Cuenta</Name><TableName>z</TableName><ColumnName>Desc_Cuenta</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>34%</ColumnWidthHTML><column_title>Descripcion Cuenta</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>TotDebito</Name><TableName></TableName><ColumnName>sum(z.TotDeb) as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>right</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>14%</ColumnWidthHTML><column_title>Total Debito</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>TotCredito</Name><TableName></TableName><ColumnNa