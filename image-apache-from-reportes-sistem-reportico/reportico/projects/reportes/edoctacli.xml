       a.referencia,
       a.ReferenciaCredito,
       a.cliente, 
       a.TipoMovimiento,
       a.fecha,
       (CASE when a.DbCr = 1 then a.TotalDocumento else 0 end) as debe,
       (CASE when a.DbCr = 0 then a.TotalDocumento else 0 end) as haber 
 from (SELECT Cliente, 
       TipoMovimiento,
       Referencia,
       TotalDocumento, 
       Fecha,     
       Descripcion, 
       1 As DbCr, 
       ' '  As ReferenciaCredito,
       'CL' as Prefi,
        Fecha as FechaDebito,
       ' ' as nombre 
 FROM  EN01DECL  WHERE 1 = 1
      [AND EN01DECL.Cliente = {proveedor}]  [ AND Fecha BETWEEN {fechadesde} AND         {fechahasta}] 
UNION SELECT DT01CRCL.Cliente, 
      DT01CRCL.TipoMovimiento, 
      DT01CRCL.ReferenciaDebito,  
      DT01CRCL.Monto,
      DT01CRCL.fecha, 
      EN01CLIE.Descripcion as Descripcion,
      0 as DbCr,     
      DT01CRCL.Referencia As ReferenciaCredito,
     'CL' as Prefi,
      EN01DECL.Vencimiento AS FechaDebito,
      EN01CLIE.DESCRIPCION 
 FROM DT01CRCL, EN01DECL  ,EN01CLIE 
WHERE  EN01DECL.cliente = EN01CLIE.codigo
   AND DT01CRCL.TipoDebito = EN01DECL.TipoMovimiento  
   AND DT01CRCL.ReferenciaDebito = EN01DECL.Referencia  
  [AND DT01CRCL.Cliente = {proveedor}] 
  [ AND DT01CRCL.Fecha BETWEEN {fechadesde} AND        {fechahasta} ]) a
  ORDER BY a.Cliente,
                    a.fecha desc,
                    a.Referencia,
                      DbCr</SQLRaw></SQL><QueryColumns><QueryColumn><Name>cliente</Name><TableName>a</TableName><ColumnName>cliente</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>CODIGO: </column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>TipoMovimiento</Name><TableName>a</TableName><ColumnName>TipoMovimiento</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>15%</ColumnWidthHTML><column_title>Tipo de Mov</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha</Name><TableName>a</TableName><ColumnName>fecha</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>35%</ColumnWidthHTML><column_title>Fecha</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>referencia</Name><TableName>a</TableName><ColumnName>re