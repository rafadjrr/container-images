<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Abono del Periodo</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><AutoPaginate>.DEFAULT</AutoPaginate><PdfZoomFactor></PdfZoomFactor><HtmlZoomFactor></HtmlZoomFactor><PageTitleDisplay>.DEFAULT</PageTitleDisplay><PageLayout>.DEFAULT</PageLayout><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT EN01CLIE.Codigo AS codigo,
       EN01CLIE.Descripcion as Descripcion, 
       DT01CRCL.Fecha as FechaCanc,      
       DT01CRCL.TipoMovimiento as TipoCanc,
       DT01CRCL.Referencia as ReferenciaCanc,   
       EN01CRCL.Descripcion as DescripcionCanc,
       EN01DECL.ZonaGeografica as Zona,  
       DT01CRCL.TipoDebito as TipoDoc,   
       DT01CRCL.ReferenciaDebito as ReferenciaDoc,
       EN01DECL.Fecha as FechaDoc, 
       EN01DECL.Vencimiento as FechaVento, 
       EN01DECL.TotalCreditos,
       EN01DECL.TotalDocumento, 
       DT01CRCL.Monto as MontoCanc,
       MT01TIMO.Codigo as CodigoTipo,
       MT01TIMO.SirveParaCancelar as SirveParaCancelar, 'CL' as Prefi,   
       Sum(DT05FACT.Cantidad) AS SumaDeCantidad 
    
      FROM  EN01CLIE, DT01CRCL, EN01CRCL  ,MT01TIMO, EN01DECL 
      INNER JOIN DT05FACT ON EN01DECL.Referencia = DT05FACT.Referencia 
      WHERE DT05FACT.UnidadMedida = 'KG' 
      GROUP BY EN01CLIE.codigo,
               EN01CLIE.descripcion,
               DT01CRCL.Fecha,
               DT01CRCL.TipoMovimiento,
               DT01CRCL.Referencia, 
               EN01CRCL.Descripcion,  
               EN01DECL.ZonaGeografica,
               DT01CRCL.TipoDebito, 
               DT01CRCL.ReferenciaDebito,
               EN01DECL.Fecha,
               EN01DECL.Vencimiento,
               EN01DECL.TotalCreditos,
               EN01DECL.TotalDocumento, 
               DT01CRCL.Monto,
               MT01TIMO.Codigo, 
               MT01TIMO.SirveParaCancelar,
               DT01CRCL.Cliente ,
               EN01DECL.TipoMovimiento,
               EN01DECL.Referencia ,
               EN01DECL.Cliente,
               EN01CRCL.TipoMovimiento,
               EN01CRCL.Referencia ,
               EN01CRCL.Cliente 
            
              
        HAVING 1 = 1  AND
   [EN01CLIE.Codigo BETWEEN {clientedesde} AND   {clientehasta}    AND]
               MT01TIMO.Codigo BETWEEN '     ' AND 'zzzzz'    AND
               DT01CRCL.Cliente = EN01CLIE.Codigo     AND 
               DT01CRCL.TipoMovimiento    = MT01TIMO.Codigo     AND 
               EN01DECL.TipoMovimiento    = DT01CRCL.TipoDebito    AND 
               EN01DECL.Referencia        = DT01CRCL.ReferenciaDebito AND
               EN01DECL.Cliente           = DT01CRCL.Cliente    AND
               EN01CRCL.TipoMovimiento    = DT01CRCL.TipoMovimiento     AND
               EN01CRCL.Referencia        = DT01CRCL.Referencia    AND 
               EN01CRCL.Cliente           = DT01CRCL.Cliente    AND
               DT01CRCL.Referencia  BETWEEN '          ' AND 'zzzzzzzzzz'    AND 
               DT01CRCL.ReferenciaDebito  BETWEEN '          ' AND 'zzzzzzzzzz'  
 [ AND DT01CRCL.Fecha BETWEEN {fechadesde} AND {fechahasta}]

ORDER BY
              EN01DECL.ZonaGeografica,
              DT01CRCL.fecha</SQLRaw></SQL><QueryColumns><QueryColumn><Name>codigo</Name><TableName></TableName><ColumnName>EN01CLIE.Codigo AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>6%</ColumnWidthHTML><column_title>Codigo</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Descripcion</Name><TableName></TableName><ColumnName>EN01CLIE.Descripcion as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>23%</ColumnWidthHTML><column_title>Nombre del Cliente</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>FechaCanc</Name><TableName></TableName><ColumnName>DT01CRCL.Fecha as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>6%</ColumnWidthHTML><column_title>Fecha</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>TipoCanc</Name><TableN