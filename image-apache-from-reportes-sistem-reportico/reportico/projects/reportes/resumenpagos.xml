         EN01CLIE.Descripcion as Descripcion, 
         DT01CRPR.Fecha as FechaCanc,  
         DT01CRPR.TipoMovimiento as TipoCanc,
         DT01CRPR.Referencia as ReferenciaCanc,    
         EN01CRPR.Descripcion as DescripcionCanc,
        DT01CRPR.TipoDebito as TipoDoc,    
         DT01CRPR.ReferenciaDebito as ReferenciaDoc,
         DT01CRPR.Monto as MontoCanc
 FROM  EN01CLIE, DT01CRPR, EN01CRPR,  EN01DEPR, MT01TIMO  
 WHERE 1 = 1 [and  EN01CLIE.Codigo BETWEEN {proveedordesde} AND {proveedorhasta} ]  AND
       MT01TIMO.Codigo BETWEEN '     ' AND 'zzzzz'    AND 
       DT01CRPR.Cliente = EN01CLIE.Codigo     AND 
       DT01CRPR.TipoMovimiento    = MT01TIMO.Codigo     AND 
       EN01DEPR.TipoMovimiento    = DT01CRPR.TipoDebito     AND 
       EN01DEPR.Referencia        = DT01CRPR.ReferenciaDebito    AND
       EN01DEPR.Cliente           = DT01CRPR.Cliente    AND
       EN01CRPR.TipoMovimiento    = DT01CRPR.TipoMovimiento     AND 
       EN01CRPR.Referencia        = DT01CRPR.Referencia    AND
       EN01CRPR.Cliente           = DT01CRPR.Cliente    AND 
       DT01CRPR.Referencia  BETWEEN '          ' AND 'zzzzzzzzzz'    AND
       DT01CRPR.ReferenciaDebito  BETWEEN '          ' AND 'zzzzzzzzzz'   
 [AND  DT01CRPR.Fecha BETWEEN {fechadesde} and {fechahasta}] 
ORDER BY
       EN01CRPR.fecha
       </SQLRaw></SQL><QueryColumns><QueryColumn><Name>codigo</Name><TableName></TableName><ColumnName>EN01CLIE.Codigo AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>8%</ColumnWidthHTML><column_title>Codigo Prov</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Descripcion</Name><TableName></TableName><ColumnName>EN01CLIE.Descripcion as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>32%</ColumnWidthHTML><column_title>Nombre</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>FechaCanc</Name><TableName></TableName><ColumnName>DT01CRPR.Fecha as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title> </column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>TipoCanc</Name><TableName></TableName><ColumnName>DT01