(SELECT SUM(EN01MVSL.Monto) FROM EN01MVSL, MT01TMBC  Where EN01MVSL.Banco = EN01BANC.codigo    AND MT01TMBC.Aumenta = 1 AND 
   (MT01TMBC.Codigo = EN01MVSL.Movimiento) [And    EN01MVSL.FechaElaboracion &lt; {fechadesde}]) AS SaldoAnteriorP, (SELECT SUM(EN01MVSL.Monto)  FROM EN01MVSL, MT01TMBC  Where EN01MVSL.Banco = EN01BANC.codigo    AND MT01TMBC.Aumenta = 0 AND    (MT01TMBC.Codigo = EN01MVSL.Movimiento) [And    EN01MVSL.FechaElaboracion &lt; {fechadesde}]) AS SaldoAnteriorN  From EN01BANC  WHERE 1 = 1 [ and (EN01BANC.Codigo Between {bancodesde} And {bancohasta})]
) a join (


SELECT EN01BANC.Codigo, EN01MVSL.Banco,EN01MVSL.Referencia, EN01MVSL.Movimiento,
  EN01MVSL.FechaElaboracion,EN01MVSL.FechaPresentacion,EN01MVSL.Concepto,
  EN01MVSL.Beneficiario, EN01MVSL.Monto , MT01TMBC.Aumenta From EN01BANC, EN01MVSL, MT01TMBC
Where 
 (EN01MVSL.Banco = EN01BANC.Codigo) [ and (EN01BANC.Codigo Between {bancodesde} And {bancohasta})]  AND 
 [(EN01MVSL.Movimiento between {movdesde} and {movhasta}) And]   (EN01MVSL.movimiento = MT01TMBC.Codigo) 
 AND 1=1 
  [ AND EN01MVSL.FechaElaboracion between {fechadesde} And  {fechahasta}  ] 
) b on (a.Codigo = b.Codigo) And (a.Codigo = b.Banco)
order by
a.Codigo,
b.FechaElaboracion,b.Referencia</SQLRaw></SQL><QueryColumns><QueryColumn><Name>CodMain</Name><TableName></TableName><ColumnName>a.Codigo as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF>Codigo</ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Codigo</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Descripcion</Name><TableName>a</TableName><ColumnName>Descripcion</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Banco</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>NroDeCuenta</Name><TableName>a</TableName><ColumnName>NroDeCuenta</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>SaldoAnterior</Name><TableName></TableName><ColumnName>a.S