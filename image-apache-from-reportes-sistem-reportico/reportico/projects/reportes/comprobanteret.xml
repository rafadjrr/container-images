EN01DEPR.FechaDocCP,
 EN01DEPR.Referencia as dt_NroFactura, 
EN01DEPR.NroControlFactura, 
EN01DEPR.NroComprobanteRet, 
EN01DEPR.TotalDocumento as dt_pagado,
 EN01DEPR.Monto as MontoOriginal, 
EN01DEPR.BaseImponible, 
EN01CLIE.descripcion as en_descripcion, 
EN01DEPR.Monto as dt_sujereten, 
EN01CLIE.rif, 
EN01CLIE.Direccion as en_direccion, 
EN01CLIE.Telefonos,
 DT01DEPR.TipoMovimiento, 
DT01DEPR.RecargoDescuento, 
DT01DEPR.monto as dt_impretenido,
MT01REDE.RecargoDescuento, 
MT01REDE.ImpuestoDefinitivo,
MT01REDE.Porcentaje
 FROM (EN01DEPR INNER JOIN EN01CLIE ON EN01DEPR.Cliente = EN01CLIE.codigo) LEFT JOIN (DT01DEPR LEFT JOIN MT01REDE ON DT01DEPR.RecargoDescuento = MT01REDE.Codigo) ON (EN01DEPR.Referencia = DT01DEPR.Referencia) AND (EN01DEPR.TipoMovimiento = DT01DEPR.TipoMovimiento) AND 
(EN01DEPR.Cliente = DT01DEPR.Cliente) WHERE (1=1) 
 [AND EN01DEPR.Fecha Between {fechadesde} And {fechahasta}] AND 
(DT01DEPR.RecargoDescuento = 'ISR')
order by EN01CLIE.descripcion</SQLRaw></SQL><QueryColumns><QueryColumn><Name>dt_Fechapago</Name><TableName></TableName><ColumnName>EN01DEPR.Fecha as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>40%</ColumnWidthHTML><column_title>Fecha Pago</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>FechaDocCP</Name><TableName>EN01DEPR</TableName><ColumnName>FechaDocCP</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>dt_NroFactura</Name><TableName></TableName><ColumnName>EN01DEPR.Referencia as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>15%</ColumnWidthHTML><column_title>Nro Factura</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>NroControlFactura</Nam