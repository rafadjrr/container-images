<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Carpeta Cliente</ReportTitle><ReportDescription>Reporte muestra los datos del cliente, sus pedidos y los pagos asociados.</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><AutoPaginate>.DEFAULT</AutoPaginate><PdfZoomFactor></PdfZoomFactor><HtmlZoomFactor></HtmlZoomFactor><PageTitleDisplay>.DEFAULT</PageTitleDisplay><PageLayout>.DEFAULT</PageLayout><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT EN01CLIE.ruta AS Division, 
EN01FACT.CodZona, 
EN01FACT.CodMovimiento,
EN01FACT.Referencia,
EN01FACT.FechaMov, 
EN01CLIE.estado,
DT01PAIS.Descripcion, 
EN01FACT.CodClienteProveedor, 
EN01CLIE.descripcion,
EN01CLIE.telefonos, 
EN01CLIE.fechaDeRegistro, 
EN01CLIE.segmento, 
DT05CLIE.tipo, 
MT01ZOGE.Descripcion AS DescripcionZona, DT01CRCL.TipoMovimiento, 
DT01CRCL.Referencia,
DT01CRCL.TipoDebito, 
DT01CRCL.ReferenciaDebito,
DT01CRCL.Monto, 
EN01FACT.MovReferenciado,
EN01CLIE.rif, 
EN01CLIE.direccion,
EN01CLIE.nit, 
EN01CLIE.limiteDeCredito, 
EN01FACT.CodVendedor, EN01FACT.CantidadDias, EN01FACT.TipoPago, 
EN01DECL.Vencimiento as fechavencimiento ,
EN01DECL.TotalCreditos, 
DT01CRCL.Fecha,
EN01DECL.TotalDocumento,
DATEDIFF(day,EN01DECL.Vencimiento,DT01CRCL.Fecha) as diastrans
FROM (((((EN01CLIE LEFT JOIN DT01PAIS ON (EN01CLIE.pais = DT01PAIS.codigo) AND (EN01CLIE.estado = DT01PAIS.Estado)) LEFT JOIN DT05CLIE ON EN01CLIE.codigo = DT05CLIE.codigo) RIGHT JOIN EN01FACT ON EN01CLIE.codigo = EN01FACT.CodClienteProveedor) LEFT JOIN MT01ZOGE ON EN01FACT.CodZona = MT01ZOGE.codigo)  JOIN EN01DECL ON (EN01FACT.CodClienteProveedor = EN01DECL.Cliente) AND (EN01FACT.Referencia = EN01DECL.Referencia))  JOIN DT01CRCL ON (EN01DECL.Referencia = DT01CRCL.ReferenciaDebito) AND 
(EN01DECL.Cliente = DT01CRCL.Cliente)
GROUP BY EN01CLIE.ruta, EN01FACT.CodZona, EN01FACT.CodMovimiento, EN01FACT.Referencia, EN01FACT.FechaMov, EN01CLIE.estado, 
DT01PAIS.Descripcion, EN01FACT.CodClienteProveedor, EN01CLIE.descripcion, EN01CLIE.telefonos, EN01CLIE.fechaDeRegistro, 
EN01CLIE.segmento, DT05CLIE.tipo, MT01ZOGE.Descripcion, DT01CRCL.TipoMovimiento, DT01CRCL.Referencia, DT01CRCL.TipoDebito, 
DT01CRCL.ReferenciaDebito, DT01CRCL.Monto, EN01FACT.MovReferenciado, EN01CLIE.rif, EN01CLIE.direccion, EN01CLIE.nit, 
EN01CLIE.limiteDeCredito, EN01FACT.CodVendedor, EN01FACT.CantidadDias, EN01FACT.TipoPago, EN01DECL.Vencimiento, 
EN01DECL.TotalCreditos, DT01CRCL.Fecha, EN01DECL.TotalDocumento
HAVING 1 = 1 AND
EN01FACT.CodMovimiento IN ('VTN','VTS','VTE') [AND  EN01FACT.CodClienteProveedor = {cliente}]
ORDER BY EN01FACT.FechaMov
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Division</Name><TableName></TableName><ColumnName>EN01CLIE.ruta AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>DIVISION:</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>CodZona</Name><TableName>EN01FACT</TableName><ColumnName>CodZona</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>ZONA</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>CodMovimiento</Name><TableName>EN01FACT</TableName><ColumnName>CodMovimiento</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>8%</ColumnWidthHTML><column_title>Mov.</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Referencia</Name><TableName>DT01CRCL</TableName><ColumnName>Referenci